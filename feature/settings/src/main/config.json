{
  "app": {
    "bundleName": "com.ohos.launcher",
    "vendor": "ohos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "srcPath": "",
    "package": "com.ohos.launcher.settings",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "settings",
      "moduleType": "feature",
      "installationFree": true
    },
    "abilities": [
      {
        "visible": false,
        "name": ".MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:settings_MainAbility",
        "type": "page",
        "launchType": "singleton",
        "srcPath": "MainAbility",
        "srcLanguage": "ets"
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "pages/Settings"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}